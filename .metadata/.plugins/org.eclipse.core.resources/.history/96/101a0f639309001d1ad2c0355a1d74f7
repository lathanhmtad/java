class MyLinkedList {
	
	public class Node {
		int value;
		Node next;
		Node(int value) {
			this.value = value;
		}
	}
	
	Node head = null;
	int size = 0;
	
    public MyLinkedList() {
    	
    }
    
    public int get(int index) {
        return 0;
    }
    
    public void addAtHead(int val) {
    	Node newNode = new Node(val);
    	if(head == null) {
    		head = newNode;
    		return;
    	}
    	newNode.next = head;
    	head = newNode;
    	size++;
    }
    
    public void addAtTail(int val) {
    	size++;
    	Node newNode = new Node(val);
    	if(head == null) {
    		head = newNode;
    		return;
    	}
    	Node i = head;	 
		while(i.next != null) {
			i = i.next;
		}    	       
		i.next = newNode;
    }
    
    public void addAtIndex(int index, int val) {
        
    }
    
    public void deleteAtIndex(int index) {
        
    }
    
    public void printLinkedList() {
    	Node i = head;
    	while(head != null) {
    		
    	}
    }
}